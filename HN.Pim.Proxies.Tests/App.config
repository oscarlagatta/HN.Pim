<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.serviceModel>
    <client>
      <endpoint address="net.tcp://localhost:8009/ProductService" binding="netTcpBinding" contract="HN.Pim.Client.Contracts.IProductService" />
      <endpoint address="net.tcp://localhost:8009/ResourceMasterService" binding="netTcpBinding" contract="HN.Pim.Client.Contracts.IResourceMasterService" />
    </client>
    <bindings>
      <netTcpBinding>
        <binding maxReceivedMessageSize="2000000" transactionFlow="true">
          <reliableSession enabled="true" />
        </binding>
      </netTcpBinding>
    </bindings>
  </system.serviceModel>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>

</configuration>